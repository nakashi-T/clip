<div class="current-wrapper">
  <% posts.each do |post| %>
    <div class="post-image">
      
      <div class="inner">
        <% if post.image? %>
          <%= render "/evaluations/evaluation_posts_list", post: post %>
          <%= link_to image_tag(post.image.url), post_path(post) %>
        <% end %>
      </div>
    </div>
  <% end %>
</div>